        <datafield tag="245" ind1="1" ind2="0">
            <subfield code="a">Interpreting multiscale domains of tree cover disturbance patterns in
                North America</subfield>
        </datafield>
        <datafield tag="300" ind1=" " ind2=" ">
            <subfield code="a">p. 147-152.</subfield>
        </datafield>
        <datafield tag="506" ind1=" " ind2=" ">
